<div>
  <%= form_with model:@quiz,local: true do |f| %>
    

    <div class="field">
      <%= f.label :level %><br />
      <%= f.text_field :level  %>
    </div>

    <div class="field">
      <%= f.label :section %>
      <%= f.text_field :section %>
    </div>

    <div class="field">
      <%= f.label :title %>
      <%= f.text_field :title %>
    </div>

    <div class="field">
      <%= f.label :question %>
      <%= f.text_field :question %>
    </div>

    <div class="field">
      <%= f.label :choice1 %>
      <%= f.text_field :choice1 %>
    </div>

    <div class="field">
      <%= f.label :choice2%>
      <%= f.text_field :choice2 %>
    </div>

    <div class="field">
      <%= f.label :choice3 %>
      <%= f.text_field :choice3 %>
    </div>

    <div class="field">
      <%= f.label :choice4 %>
      <%= f.text_field :choice4 %>
    </div>



    <div class="actions">
      <%= f.submit "Register" %>
    </div>
  <% end %>
</div>
