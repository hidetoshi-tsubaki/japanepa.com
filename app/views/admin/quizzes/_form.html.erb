<div class="form_wrapper">
  <%= form_with model: [:admin, quiz], builder: CustomFormBuilder do |f| %>
    <% if category.present? %>
      <%= f.hidden_field :category_id, value: category.id %>
    <% end %>
    <div class="field">
      <%= f.label :question %>
      <%= f.text_field :question %>
    </div>
    <div class="field">
      <%= f.label :choice1 %>
      <%= f.text_field :choice1 %>
    </div>
    <div class="field">
      <%= f.label :choice2%>
      <%= f.text_field :choice2 %>
    </div>
    <div class="field">
      <%= f.label :choice3 %>
      <%= f.text_field :choice3 %>
    </div>
    <div class="field">
      <%= f.label :choice4 %>
      <%= f.text_field :choice4 %>
    </div>
    <div class="form_btn">
      <%= f.submit "作成", id: 'form_btn',data: { confirm: "送信しますか？"} %>
    </div>
  <% end %>
</div>